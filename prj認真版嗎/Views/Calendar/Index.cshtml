@using prj認真版嗎.ViewModel
@model List<C行事曆>


<link href="~/lib/jian/main.css" rel="stylesheet" />
<link href="~/css/JianCalendar.css" rel="stylesheet" />
<div>
    <div class="container" >
        <div id='calendar' >

        </div>
    </div>
</div>
<script>

    document.addEventListener('DOMContentLoaded', function () {
        var qq = [];

        var qw = {
            title: '1213',
            start: '2022-11-04',
            end: '2022-11-06',
        }
        qq.push(qw);
        var calendarEl = document.getElementById('calendar');
        var calendar = new FullCalendar.Calendar(calendarEl, {
            initialView: 'dayGridMonth',
            locale: 'zh-tw',
            navLinks: true,
            headerToolbar: {
                left: 'prev,next today',
                center: 'title',
                right:'dayGridMonth,timeGridWeek,timeGridDay'
            },
            events:qq
        });
        calendar.render();
    });

</script>

<script src="~/lib/jian/main.js"></script>
<script src="~/lib/jian/locales-all.js"></script>




